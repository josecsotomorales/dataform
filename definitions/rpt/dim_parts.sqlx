config {
  type: "incremental",
  schema: "rpt",
  uniqueKey: ["p_partkey"],
  assertions: {
    uniqueKey: ["p_partkey"],
    nonNull: ["p_partkey", "p_name"],
  }
}

select 
  p_partkey,
  p_name,
  p_mfgr,
  p_brand,
  p_type,
  p_size,
  p_container,
  p_retailprice,
  p_comment
from ${ ref('stg_part') }