config {
  type: "incremental",
  schema: "rpt",
  uniqueKey: ["c_custkey"],
  assertions: {
    uniqueKey: ["c_custkey"],
    nonNull: ["c_custkey", "c_name"],
  }
}

select
    c_custkey,
    c_name,
    c_address,
    c_nationkey,
    c_phone,
    c_acctbal,
    c_mktsegment,
    c_comment,
    n_name,
    r_name
from ${ ref('stg_customers') }